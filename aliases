alias ai-summarise='bash ~/.local/bin/ai-summarise'
alias bash-new='bash ~/.local/bin/bash-new'
alias edit='nix-edit'
alias l='ls -alh'
alias ll='ls -l'
alias ls='ls --color=tty'
alias o='nvim'
alias qwik-asteroid='ssh kain@elnath.uberspace.de'
alias qwik-backup='rsync -r --progress /home/kain/* /run/media/kain/files/'
alias qwik-edit='sudo nvim -c :120 /etc/nixos/configuration.nix'
alias qwik-export='sudo cp /etc/nixos/configuration.nix /run/media/kain/configuration'
alias qwik-import='sudo cp /run/media/kain/configuration/configuration.nix /etc/nixos/'
alias qwik-test='sudo nixos-rebuild test'
alias rebuild='sudo nixos-rebuild switch'
